	<!-- Colonist tales -->
	<DissectedColonistWithPassionTale.label>Leiche seziert</DissectedColonistWithPassionTale.label>
	<DissectedColonistWithPassionTale.rulePack.rulesStrings>
		<li>tale_noun->[DISSECTOR_nameDef] seziert [CORPSE_nameDef]</li>
		<li>tale_noun->[CORPSE_nameDef] wird von [DISSECTOR_nameDef] seziert</li>
		<li>tale_noun->[CORPSE_nameDef]s Sezierung</li>
		<li>image->[DISSECTOR_nameFull], wie [DISSECTOR_pronoun] [CORPSE_nameFull] [circumstance_group] seziert</li>
		<li>image->[DISSECTOR_nameFull], wie [DISSECTOR_pronoun] [CORPSE_nameFull]s Körper [circumstance_group] aufschneidet</li>
		<li>image->[DISSECTOR_nameFull], wie [DISSECTOR_pronoun] [CORPSE_nameFull]s Herz [circumstance_group] hält</li>
		<li>circumstance_phrase->mit professioneller Effizienz</li>
		<li>circumstance_phrase->mit Trauer</li>
		<li>circumstance_phrase->mit gemischten Gefühlen</li>
		<li>desc_sentence->[DISSECTOR_nameShortDef] schaut [CORPSE_nameShortDef]s Leiche mit Trauer an.</li>
		<li>desc_sentence->[CORPSE_nameShortDef] ist in Stücke geschnitten.</li>
		<li>desc_sentence->[CORPSE_nameShortDef] fehlen ein paar Körperteile.</li>
	</DissectedColonistWithPassionTale.rulePack.rulesStrings>
	
